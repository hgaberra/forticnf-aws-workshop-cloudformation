				},
				"Tags": [
					{
						"Key": "Name",
						"Value": {
							"Fn::Join": [
								"-",
								[
									{
										"Ref": "AWS::StackName"
									},
									"GwlbRouteTable"
								]
							]
						}
					}
				]
			}
		},
		"IgwRouteTable": {
			"Type": "AWS::EC2::RouteTable",
